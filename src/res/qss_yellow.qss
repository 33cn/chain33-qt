QPushButton {
    background-color: #575757;
    color:#cbcbcb;
    border-radius: 4px;
    border: none;
    padding-left: 13px;
    padding-right: 13px;
    height: 20px;
}

QPushButton:hover{
    background-color: #686868;
}

QPushButton:pressed {
    color: #333333;
    background: #ffba26;
}

QPushButton:!enabled{
    color: #808080;
    background: #464646;
}

QLineEdit {
    background-color: #2c2c2c;
    border-radius: 4px;
    border: none;
    height: 20px;
}

QLineEdit:!enabled {
    color: #808080;
    background: #464646;
}

QToolBar {
    background-color:#3d3d3d;
    border: none;
    color: #FFFFFF;
    padding: 3px;
}

QToolBar::separator {
    width: 20px;
}

QListView {
    border: none;
}

QComboBox {
    background-color: #2c2c2c;
    border-radius: 4px;
    border: 1px solid #575757;
    height: 20px;
}

QComboBox::drop-down {
    width: 24px;
    border: none;
    background: transparent;
}

QComboBox::drop-down:hover { background: rgba(255, 255, 255, 30); }

QComboBox::down-arrow { image: url(:/icons/downarrow); }

QDateEdit {
    background: transparent;
    border-radius: 4px;
    border: none;
    width: 70px;
    height: 20px;
}

QMenu { background-color:#2c2c2c; }

QMenu::item:selected { background-color:#454545; }

QMenuBar { background-color:#2c2c2c; }

QMenuBar::item { background-color:#2c2c2c; }

QMenuBar::item:selected { background-color:#575757; }

QMenuBarExtension { background-color:#2c2c2c; }

QProgressBar {
    background-color: #575757;
    border-radius: 2px;
}

QProgressBar::chunk {
    background-color: #ffba26;
    border-radius: 2px;
}

QPushButton#sendButton, QPushButton#clearButton {
    background-color: #575757;
    border-radius: 4px;
    border: none;
    height: 30px;
}

QPushButton:hover#sendButton, QPushButton:hover#clearButton{
    background-color: #686868;
}

QPushButton:pressed#sendButton, QPushButton:pressed#clearButton {
    color: #333333;
    background: #ffba26;
}

QToolButton {
    background-color: #2c2c2c;
    border-radius: 4px;
    border: none;
    height: 20px;
}

QTextEdit#messagesWidget {
    background-color: #2c2c2c;
    border-radius: 4px;
    border: none;
    padding: 10px;
}

QTextEdit#detailText {
    background-color: #2c2c2c;
    border-radius: 4px;
    border: none;
    padding: 10px;
    color: #ffffff;
}

/* seed */
QLabel#manage_label {
    background: transparent;
    color: #ffba26;
    font: 30px;
}

QLabel#manage_label2 {
    background: transparent;
    color: #ffba26;
    font: 20px;
}

QTabWidget::pane {
    border: none;
}

QTabWidget::tab-bar {
    border: none;
}

QTabBar::tab {
    min-width: 100px;
    height: 30px;
    border: 1px solid #ffba26;
    border-radius: 0px;
    color: #ffba26;
}

QTabBar::tab:selected {
    color: #2c2c2c;
    background: #ffba26;
    border-radius: 0px
}

QPushButton#UpDataBtn, QPushButton#SeedNextBtn, QPushButton#SeedPrevBtn, QPushButton#SeedCreateWalletBtn, QPushButton#SeedClearBtn {
    background-color: #575757;
    color:#ffffff;
    border-radius: 4px;
    border: none;
}

QPushButton:pressed#UpDataBtn, QPushButton:pressed#SeedNextBtn, QPushButton:pressed#SeedPrevBtn, QPushButton:pressed#SeedCreateWalletBtn, QPushButton:pressed#SeedClearBtn {
    color: #333333;
    background: #ffba26;
}

QLineEdit#SeedPsdEdit, QLineEdit#SeedSurePsdEdit{
    background-color: #202020;
    border-radius: 4px;
    border: none;
    height: 30px;
    padding-left: 10px;
}

QTableView {
    color: #808080;
    selection-background-color: rgb(77,77,77);        /*选中区域的背景色*/
    padding: 0px;
    border: none;
}

QTableView#tableToken, QTableView#tableAsset {
    color: #808080;
    gridline-color: #353535;                              /*表格内框颜色 表格线颜色*/
    selection-color: white;                             /*选中区域的文字颜色*/
    selection-background-color: rgb(77,77,77);        /*选中区域的背景色*/
    padding: 0px;
    border: none;
}

QTableView::item#tableToken,QTableView::item#tableAsset {
    color: rgb(153,153,153);
}

QTableView#listSymbol {
    color: #808080;
    gridline-color: #353535;                              /*表格内框颜色 表格线颜色*/
    selection-color: #ffba26;                             /*选中区域的文字颜色*/
    selection-background-color: rgb(77,77,77);        /*选中区域的背景色*/
    padding: 0px;
    border: none;
}

QHeaderView::section {
    background-color:transparent;
    color: #ffffff;
    padding: 0px;
    border: none;
    height:34;
    border-bottom: 1px solid #2c2c2c;
}

/**********滚动条-垂直**********/
QScrollBar:vertical {
    width: 10px;
    background: transparent;
    margin-left: 0px;
    margin-right: 2px;
    border-radius: 2px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar::handle:vertical {
    width: 5px;
    min-height: 30px;
    background: #575757;
    margin-top: 2px;
    margin-bottom: 2px;
    border-radius: 4px;
}

QScrollBar::handle:vertical:hover {
    background: #686868;
}

QScrollBar::sub-line:vertical {
    height: 0px;
    background: none;
    subcontrol-position: top;
}

QScrollBar::add-line:vertical {
    height: 0px;
    background: none;
    subcontrol-position: bottom;
}

QRadioButton {
    spacing: 9px;
}

QRadioButton::indicator {
    width: 14px;
    height: 14px
}

QRadioButton::indicator:checked {
    image: url(:/icons/yellow_radio_checked);
}

QRadioButton::indicator:unchecked {
    image: url(:/icons/yellow_radio_unchecked);
}

QFrame#line1px, QFrame#line_2_1px { background-color: #2c2c2c; height: 1px; }

QWidget#sendwidget1_0, QWidget#sendwidget1_1, QWidget#sendwidget1_2, QWidget#sendwidget1_3 {background-color:#2c2c2c;border-radius: 4px;}

QLabel { background: transparent; }

QValidatedLineEdit:!enabled#fromAddr { background: transparent; }



